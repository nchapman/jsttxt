<div class="note">
  <h2 class="title"><%=h @note.title %></h2>
  <% if @note.tag_list.size > 0 %>
    <div class="tags">
      <% @note.tag_list.each do |tag| %>
        <span><%= tag %></span>
      <% end %>
    </div>
  <% end %>
  <div id="note_body">
    <%= @note.body_html %>
  </div>
</div>

<script>
  jsttxt.applyTextFilters();
</script>

<%= link_to 'Edit', edit_note_path(@note) %> |
<%= link_to 'Back', notes_path %>